algoritmo "pgm38"
// Função : atribui valor e modifica
// Autor :  renato m. dias
// Data : 25/01/2021
// Seção de Declarações
var
         a,b,c:real
inicio
         // Seção de Comandos
         // atribui valor
         a <- 10
         b <- 20
         c <- a
         b <- c
         a <- b
         //apresenta na tela
         escreval("A,B,C",a,b,c)
fimalgoritmo
