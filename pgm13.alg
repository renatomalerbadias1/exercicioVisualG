algoritmo "pgm13"
// Função : exponenciação de dois numeros digitados pelo usuario
// Autor :   Renato Mdias
// Data : 21/01/2021
// Seção de Declarações
var
       num1,num2,res:real
inicio

     //atribuição de valores
        num1 <-6
        num2 <-2
        


      //processamento
      res <- num1 ^ num2
      //resposta
      escreval("A Exponenciação é :",res)

fimalgoritmo
