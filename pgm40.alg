algoritmo "pgm40"
// Função : atribui e modifica valore
// Autor :  renato m. dias
// Data : 25/01/2021
// Seção de Declarações
var
         a,b,c:real
inicio
         // Seção de Comandos
         //atreibuição de valores a variavel
         a <- 10
         b <- 5
         c <-a+b
         b <- 20
         a <- 10
         //apresenta na tela
         escreval("A,B,C",a,b,c)

fimalgoritmo
